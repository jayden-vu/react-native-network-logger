{
  "name": "@nextgen/rn-network-logger",
  "description": "An HTTP traffic monitor for React Native with zero dependencies",
  "private": true,
  "version": "1.0.0",
  "base-repository": {
    "type": "git",
    "url": "https://github.com/alexbrazier/react-native-network-logger.git",
    "version": "2.0.1"
  },
  "main": "src/index",
  "react-native": "src/index",
  "codegenConfig": {
    "name": "RNComponentsSpec",
    "type": "modules",
    "jsSrcsDir": "src"
  },
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "yarn eslint && yarn prettier",
    "eslint": "eslint \"**/*.{js,ts,tsx}\"",
    "prettier": "prettier --check '{,example/}src/**/*.{js,json,ts,tsx}' '{*,example/*}.{js,ts,tsx}'",
    "prepare": "bob build",
    "release": "release-it",
    "bootstrap": "yarn"
  },
  "devDependencies": {
    "@nextgen/rn-config": "workspace:*",
    "@nextgen/rn-ui": "workspace:*",
    "@types/react": "^18.2.6",
    "react-native": "^0.77.0"
  },
  "peerDependencies": {
    "@nextgen/rn-ui": "*"
  }
}
